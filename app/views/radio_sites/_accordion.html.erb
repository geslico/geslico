                <table>
                    <thead class="color-cabecera">
                        <tr>                                                 
                            <th>Marca</th>
                            <th>Modelo</th>
                            <th>Frigorías(KW)</th>
                            <th>Tipo</th>                                    
                            <th>Potencia(VCA)</th>
                            <th>Módulo control</th>
                            <th>Freecooling</th>                                    
                            <th>Gas</th>                                
                            <th>Carga(kg)</th>
