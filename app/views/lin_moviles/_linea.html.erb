<li class="accordion-item is-active" data-accordion-item>
  <a href="#" class="accordion-title">Línea</a>
    <div class="accordion-content" data-tab-content>     
      <div class="row">                             
        <div class="small-12 medium-4 large-4 columns">
          <%= f.label :cPerfil %>              
          <%= f.select :cPerfil , options_from_collection_for_select(PerfilesMovil.all, "cPerfil", "cPerfil", @lin_movil.cPerfil), {}, {:disabled => true} %>
        </div>
        <div class="small-12 medium-4 large-4 columns">
          <%= f.label :cPerfilAutorizado %>                
          <%= f.select :cPerfilAutorizado , options_from_collection_for_select(PerfilesMovil.all, "cPerfil", "cPerfil", @lin_movil.cPerfilAutorizado), {}, {:disabled => @readonly} %>
        </div>
        <div class="small-12 medium-4 large-4 columns">
          <%= f.label :cCoberturaNormativa %>                
          <%= f.select :cCoberturaNormativa , options_from_collection_for_select(PerfilesMovil.select(:cCobertura).distinct, "cCobertura", "cCobertura", @lin_movil.cCoberturaNormativa), {:include_blank => true }, {:disabled => @readonly} %>
        </div>
      </div>
      <div class="row">                                     
        <div class="small-12 medium-4 large-3 columns">
          <%= f.label :cUsuarioIntranet %>              
          <%= f.text_field :cUsuarioIntranet, :readonly => @readonly %>
        </div>
        <div class="small-12 medium-4 large-2 columns">
          <%= f.label :nNumFaxAsoc %>              
          <%= f.telephone_field :nNumFaxAsoc, :readonly => @readonly %>
        </div>
        <div class="small-12 medium-4 large-2 columns">
          <%= f.label :cIPm2m %>              
          <%= f.text_field :cIPm2m, :readonly => @readonly %>
        </div>        
    </div>
    <div class="row">                             
      <div class="small-6 medium-2 large-2 columns">
          <%= f.label :cInternet %>              
          <%= f.select :cInternet, options_for_select([["",nil],["SÍ","1"], ["NO", "0"]], @lin_movil.cInternet==nil ? nil: @lin_movil.cInternet.strip), {}, {:disabled => true} %>
          <%= f.select :cInternetG, options_for_select([["",nil],["SÍ","1"], ["NO", "0"]], @lin_movil.cInternetG==nil ? nil: @lin_movil.cInternetG.strip), {}, {:disabled => @readonly} %>
      </div>    
      <div class="small-6 medium-2 large-2 columns">
          <%= f.label :cIntranet %>              
          <%= f.select :cIntranet, options_for_select([["",nil],["SÍ","1"], ["NO", "0"]], @lin_movil.cIntranet==nil ? nil: @lin_movil.cIntranet.strip), {}, {:disabled => true} %>
          <%= f.select :cIntranetG, options_for_select([["",nil],["SÍ","1"], ["NO", "0"]], @lin_movil.cIntranetG==nil ? nil: @lin_movil.cIntranetG.strip), {}, {:disabled => @readonly} %>
      </div>    
      <div class="small-6 medium-2 large-2 columns">
          <%= f.label :cDual %>              
          <%= f.select :cDual, options_for_select([["",nil],["SÍ","1"], ["NO", "0"]], @lin_movil.cDual==nil ? nil: @lin_movil.cDual.strip), {}, {:disabled => true} %>
          <%= f.select :cDualG, options_for_select([["",nil],["SÍ","1"], ["NO", "0"]], @lin_movil.cDualG==nil ? nil: @lin_movil.cDualG.strip), {}, {:disabled => @readonly} %>
      </div>            
      <div class="small-6 medium-2 large-2 columns">
          <%= f.label :cMultisim %>              
          <%= f.select :cMultisim, options_for_select([["",nil],["SÍ","1"], ["NO", "0"]], @lin_movil.cMultisim==nil ? nil: @lin_movil.cMultisim.strip), {}, {:disabled => true} %>
          <%= f.select :cMultisimG, options_for_select([["",nil],["SÍ","1"], ["NO", "0"]], @lin_movil.cMultisimG==nil ? nil: @lin_movil.cMultisimG.strip),  {}, {:disabled => @readonly} %>
      </div>
      <div class="small-12 medium-4 large-4 columns">
          <%= f.label :cObserva %>              
          <%= f.text_area :cObserva, rows: 3, :readonly => @readonly%>
      </div>
  </div>
  </div>
</li> 